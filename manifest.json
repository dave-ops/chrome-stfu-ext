{
    "manifest_version": 3,
    "name": "chrome-stfu-ext",
    "version": "1.0",
    "description": "Mutes volume during commercials and annoying people while streaming",
    "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*", "*://*.cnbc.com/*"], // Add more sites
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}